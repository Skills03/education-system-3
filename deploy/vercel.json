{
  "version": 2,
  "name": "interactive-learning-platform",
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/concept",
      "dest": "/concept.html"
    },
    {
      "src": "/project",
      "dest": "/project.html"
    },
    {
      "src": "/visual",
      "dest": "/visual.html"
    }
  ]
}
